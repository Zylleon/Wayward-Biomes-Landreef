<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="PlantBase">
		<defName>ZWB_ValentineTrumpets</defName>
		<label>valentine trumpets</label>
		<description>A small type of tubular coral that grows in small clusters. It is praised and sought after for its beauty. Many will often give a bouquet of these to their loved ones as gifts.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Beauty>14</Beauty>
			<BeautyOutdoors>14</BeautyOutdoors>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>ZWB_Textures/Plants/ValentineTrumpets</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowOrSandDepth>0.5</hideAtSnowOrSandDepth>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilityMin>0.05</fertilityMin>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.2~0.55</visualSizeRange>
			<topWindExposure>0.2</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>150</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<purpose>Beauty</purpose>
		</plant>
	</ThingDef>


